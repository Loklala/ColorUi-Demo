
.deposit-btn{
	width: 100%;
}
.show{
	display: block;
}
.hide{
	display: none;
}
.tips{
	/* text-indent:5upx; */
	color: red;
}
.tip-text{
	color: #444444;
	text-indent:20%;
	display: block;
	width: 100%;
}
.edit-btn{
	background: -webkit-gradient(linear,left top, right top,from(#1cbbb4),to(#1cbbb4));
	background: -o-linear-gradient(left,#1cbbb4,#1cbbb4);
	background: linear-gradient(to right,#1cbbb4,#1cbbb4);
	font-size: 25rpx;
	margin-right: 15rpx;
	color: #555555;
}
.bg{
	background: -webkit-gradient(linear,left top, right top,from(#4dbf92),to(#1cbbb4));
	background: -o-linear-gradient(left,#4dbf92,#1cbbb4);
	background: linear-gradient(to right,#4dbf92,#1cbbb4);
}
.menu{
	margin-top: 30rpx;
}
.btn{
	background: -webkit-gradient(linear,left top, right top,from(#3396e9),to(#1ab7b9));
	background: -o-linear-gradient(left,#3396e9,#1ab7b9);
	background: linear-gradient(to right,#3396e9,#1ab7b9);
	width: 96%;
	margin-left: 2%;
	margin-top: 50rpx;
}
.page {
	height: 100Vh;
	width: 100vw;
}
.title{
	color: #777777;
	text-align: left;
	width: 160rpx;
}
.icon-title{
	color: #777777;
	text-align: left;
	width: 60rpx;
}
.btn-bg{
	background: #fff;
	color: #007AFF;
}


